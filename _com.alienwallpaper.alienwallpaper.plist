<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.alienwallpaper.alienwallpaper</string>
	<key>ProgramArguments</key>
	<array>
    <string>/usr/local/bin/python3 /usr/local/bin/alien_wallpaper.py</string>
		<string>--subreddits</string>
		<string>$subreddits</string>
		<string>--multireddit</string>
		<string>$multireddit</string>
		<string>--out</string>
		<string>$out</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/com.alienwallpaper.alienwallpaper.err</string>
	<key>StandardOutPath</key>
	<string>/tmp/com.alienwallpaper.alienwallpaper.out</string>
</dict>
</plist>
